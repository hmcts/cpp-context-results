{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Event schema for notifying informant register document to the prosecuting authority",
  "id": "http://justice.gov.uk/results/courts/informant-register-notified.json",
  "type": "object",
  "properties": {
    "recipients": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/informantRegisterDocument/informantRegisterRecipient.json#"
      }
    },
    "fileId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "prosecutionAuthorityId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "templateId": {
      "type": "string"
    }
  },
  "additionalProperties": false
}