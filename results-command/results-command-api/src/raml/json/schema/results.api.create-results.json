{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/hearing/courts/results.api.create-results.json",
  "type": "object",
  "properties": {
    "session": {
      "$ref": "http://justice.gov.uk/core/courts/baseStructure.json"
    },
    "cases": {
      "type": "array",
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/caseDetails.json"
      }
    },
    "jurisdictionType": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/jurisdictionType"
    }
  },
  "additionalProperties": true,
  "required": [
    "session",
    "cases"
  ]
}