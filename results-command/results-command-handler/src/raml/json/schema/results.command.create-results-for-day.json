{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/hearing/courts/results.command.create-results-for-day.json",
  "type": "object",
  "properties": {
    "session": {
      "$ref": "http://justice.gov.uk/core/courts/baseStructure.json"
    },
    "cases": {
      "type": "array",
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/caseDetails.json"
      }
    },
    "jurisdictionType": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/jurisdictionType"
    },
    "hearingDay": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/datePattern"
    },
    "courtApplications": {
      "type": "array",
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/courtApplication.json"
      }
    }
  },
  "additionalProperties": true,
  "required": [
    "session",
    "cases",
    "hearingDay"
  ]
}