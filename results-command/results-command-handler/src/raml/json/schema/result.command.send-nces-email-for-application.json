{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://cpp.moj.gov.uk/hearing/json/schema/event/public.hearing.nces-email-notification-for-application.json",
  "type": "object",
  "properties": {
    "applicationType": {
      "type": "string"
    },
    "masterDefendantId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "listingDate" : {
      "type": "string"
    },
    "caseUrns" : {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    },
    "hearingCourtCentreName": {
      "type": "string"
    }
  },
  "required": [
    "applicationType",
    "masterDefendantId",
    "listingDate",
    "caseUrns"
  ]
}