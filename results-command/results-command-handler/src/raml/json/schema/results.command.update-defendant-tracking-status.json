{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/hearing/courts/results.command.update-defendant-tracking-status.json",
  "type": "object",
  "properties": {
    "defendantId": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "offences": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/offence.json"
      }
    }
  },
  "required": [
    "defendantId",
    "offences"
  ],
  "additionalProperties": false
}
