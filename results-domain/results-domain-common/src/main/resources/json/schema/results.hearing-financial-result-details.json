{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/hearing/courts/hearing-financial-result-details",
  "type": "object",
  "properties": {
    "hearingId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "masterDefendantId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "defendantName": {
      "type": "string"
    },
    "ncesEmail": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/email"
    },
    "offenceResultsDetails": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "#/definitions/offenceResultDetails"
      }
    }
  },
  "additionalProperties": false,
  "definitions": {
    "offenceResultDetails": {
      "type": "object",
      "properties": {
        "resultId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "hearingId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "offenceId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "amendmentReason": {
          "type": "string"
        },
        "amendmentDate": {
          "type": "string"
        },
        "isDeemedServed": {
          "type": "boolean"
        },
        "isFinancial": {
          "type": "boolean"
        },
        "isParentFlag": {
          "type": "boolean"
        },
        "resultCode": {
          "type": "string"
        },
        "impositionOffenceDetails": {
          "type": "string"
        },
        "offenceTitle": {
          "type": "string"
        },
        "dateOfResult": {
          "type": "string"
        },
        "applicationType": {
          "type": "string"
        },
        "applicationId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "applicationTitle": {
          "type": "string"
        },
        "applicationResultType": {
          "type": "string"
        },
        "createdTime": {
          "type": "string",
          "format": "date-time"
        }
      }
    }
  }
}