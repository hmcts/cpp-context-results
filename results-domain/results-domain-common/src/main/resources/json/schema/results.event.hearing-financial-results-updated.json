{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/hearing-financial-results-updated.json",
  "type": "object",
  "properties": {
    "masterDefendantId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "correlationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "hearingId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "accountNumber": {
      "type": "string"
    },
    "caseReferences": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    },
    "accountRequestTime": {
      "type": "string",
      "format": "date-time"
    },
    "createdTime": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "masterDefendantId",
    "correlationId",
    "hearingId",
    "accountNumber",
    "caseReferences",
    "accountRequestTime",
    "createdTime"
  ],
  "additionalProperties": false
}