{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/nowVariantResult.json",
  "description": "A now variant result represents a pointer to a shared result line that is required/included in the variant and pointers to the prompts (for the shared result) that are required/included in the variant",
  "type": "object",
  "properties": {
	"sharedResultId": {
	  "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
	},
	"sequence": {
	  "type": "integer"
	},
	"promptRefs": {
	  "type": "array",
	  "minItems": 1,
	  "items": {
		"$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
	  }
	}
  },
  "required": [
	"sharedResultId",
	"sequence"
  ],
  "additionalProperties": false
}
