{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/core/material.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/uuid"
    },
    "name": {
      "type": "string"
    },
    "uploadDate": {
      "$ref": "http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/datePattern"
    },
    "generationStatus": {
      "type": "string"
    },
    "userGroups": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "id",
    "userGroups"
  ],
  "additionalProperties": false
}