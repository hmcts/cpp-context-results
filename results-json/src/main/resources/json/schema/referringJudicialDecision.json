{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/referringJudicialDecision.json",
  "type": "object",
  "properties": {
    "location": {
      "description": "The location where the referring judicial decision was made",
      "type": "string"
    },
    "judiciary": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/referringJudiciary.json"
      }
    }
  },
  "required": [
    "location",
    "judiciary"
  ],
  "additionalProperties": false
}