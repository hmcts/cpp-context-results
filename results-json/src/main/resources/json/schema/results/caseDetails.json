{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/caseDetails.json",
  "type": "object",
  "properties": {
    "caseId": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "urn": {
      "type": "string"
    },
    "defendants": {
      "type": "array",
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/caseDefendant.json"
      }
    },
    "prosecutionAuthorityCode": {
      "type": "string"
    },
    "originatingOrganisation": {
      "type": "string"
    }
  },
  "additionalProperties": true,
  "required": [
    "caseId",
    "urn",
    "defendants",
    "prosecutionAuthorityCode"
  ]
}
