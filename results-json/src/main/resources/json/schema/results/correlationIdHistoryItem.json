{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/correlationIdHistoryItem.json",
  "type": "object",
  "properties": {
    "accountNumber": {
      "type": "string"
    },
    "accountCorrelationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "accountDivisionCode": {
      "type": "string"
    },
    "prosecutionCaseReferences": {
      "description": "ProsecutionCaseReferences for this item",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "createdTime": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "accountCorrelationId",
    "accountDivisionCode",
    "createdTime"
  ],
  "additionalProperties": false
