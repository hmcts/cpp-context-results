{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/referredProsecutionCase.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "prosecutionCaseIdentifier": {
      "$ref": "http://justice.gov.uk/core/courts/prosecutionCaseIdentifier.json"
    },
    "originatingOrganisation": {
      "type": "string"
    },
    "initiationCode": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/initiationCode"
    },
    "statementOfFacts": {
	  "type": "string"
	},
	"statementOfFactsWelsh": {
	  "type": "string"
	},
    "defendants": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/referredDefendant.json"
      }
    }
  },
  "required": [
    "id",
    "prosecutionCaseIdentifier",
    "initiationCode",
    "defendants"
  ],
  "additionalProperties": false
}