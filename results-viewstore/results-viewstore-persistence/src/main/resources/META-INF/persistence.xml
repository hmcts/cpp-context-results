<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">

    <persistence-unit name="results-persistence-unit" transaction-type="JTA">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <jta-data-source>java:/DS.results</jta-data-source>
        <class>uk.gov.moj.cpp.results.persist.entity.HearingResult</class>
        <class>uk.gov.moj.cpp.results.persist.entity.ResultPrompt</class>
        <class>uk.gov.moj.cpp.results.persist.entity.HearingDefendant</class>
        <class>uk.gov.moj.cpp.results.persist.entity.Hearing</class>
    </persistence-unit>

</persistence>