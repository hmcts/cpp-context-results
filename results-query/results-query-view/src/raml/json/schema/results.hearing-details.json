{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": [
    "object",
    "null"
  ],
  "properties": {
    "hearing": {
      "$ref": "http://justice.gov.uk/json/schemas/core/sharedHearing.json"
    },
    "variants": {
      "type": "array",
      "minItems": 1,
      "items": {
