  "properties": {
    "masterDefendantId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "accountCorrelationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "hearingId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "accountNumber": {
      "type": "string"
    },
    "caseReferences": {
      "type": "string"
    },
    "accountRequestTime": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": [
    "masterDefendantId",
    "accountCorrelationId",
    "hearingId",
    "caseReferences",
